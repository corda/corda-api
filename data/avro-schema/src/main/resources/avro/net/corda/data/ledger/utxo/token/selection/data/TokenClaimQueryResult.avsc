  "fields": [
      {
        "name": "poolKey",
        "type": "net.corda.data.ledger.utxo.token.selection.key.TokenCachePoolKey",
        "doc": "Key for the pool the tokens were claimed from"
      },
      {
        "name": "resultType",
        "type": {
          "name": "TokenClaimResultStatus",
          "type": "enum",
          "symbols": ["SUCCESS","NONE_AVAILABLE","AVAILABLE_CLAIMED"]
        },
        "doc": "The type of result for the claim query"
      },
      {
        "name": "claimedTokens",
        "type": {
          "type": "array",
          "items": "net.corda.data.services.data.Token",
          "doc": "List of tokens in this claim"
         }
      }
  ]
}